package com.topkst.beacon.sb3_DAO;

import org.mybatis.spring.SqlSessionTemplate;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import com.topkst.sb3_DTO.Beacon_scan;

@Repository
public class beaconDAOImpl implements beaconDAO {
	@Autowired
	   private SqlSessionTemplate sqlSession;
	
	
	 public void scanDao(Beacon_scan beacon_scan) {
	      System.out.println("impl scanDao");
	      sqlSession.insert("com.topkst.sb3.dao.mapper.beacon_scan", beacon_scan);      
}
}