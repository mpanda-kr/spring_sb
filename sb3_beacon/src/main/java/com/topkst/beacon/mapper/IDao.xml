<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.topkst.beacon.mapper">

<insert id = "beacon_scan" parameterType="com.topkst.beacon.DTO.Beacon_scan">
    insert into bluetooth_scan_log_sv (createdate,createtime,mac_address,uuid,major,minor,rssi,tx_power)values 
	(#{createdate},#{createtime},#{mac_address},#{uuid},#{major},#{minor},#{rssi},#{tx_power});
 </insert>

<select id="center_enrollList" parameterType="java.util.Map" resultType="com.topkst.beacon.DTO.Enroll_beacon">
    SELECT * FROM smartbus3_demo.beacon where center_code = #{center_id};
</select>

</mapper>
